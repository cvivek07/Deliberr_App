            android:layout_width="210dp"
            android:layout_height="19dp"
            android:layout_marginEnd="32dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:gravity="end"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/wikipediarocket" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:textColor="@android:color/black"
            android:text="Engine Version"
            app:layout_constraintBaseline_toBaselineOf="@+id/enginever"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/enginever"
            android:layout_width="210dp"
            android:layout_height="20dp"
            android:layout_marginEnd="32dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:gravity="end"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/enginetype" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:text="Landing Legs Number"
            android:textColor="@android:color/black"
            app:layout_constraintBaseline_toBaselineOf="@+id/legsnumber"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/legsnumber"
            android:layout_width="196dp"
            android:layout_height="18dp"
            android:layout_marginEnd="32dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:gravity="end"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/enginever" />

        <TextView
            android:id="@+id/textView39"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="16dp"
            android:text="Landing Legs Material"
            android:textColor="@android:color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/legsmat"
            app:layout_constraintTop_toBottomOf="@+id/textView37" />

        <TextView
            android:id="@+id/legsmat"
            android:layout_width="194dp"
            android:layout_height="19dp"
            android:layout_marginEnd="32dp"
            android:layout_marginTop="16dp"
            android:gravity="end"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/legsnumber" />
    </android.support.constraint.ConstraintLayout>


</ScrollView>

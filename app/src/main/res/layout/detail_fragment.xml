<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
    android:layout_marginBottom="80dp"
        android:fillViewport="true">

    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Flight Number"
            android:textColor="@android:color/black"
            app:layout_constraintBaseline_toBaselineOf="@+id/flightnumber"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/flightnumber"
            android:layout_width="234dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginTop="16dp"
            android:gravity="end"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Mission Name"
            android:textColor="@android:color/black"
            app:layout_constraintBaseline_toBaselineOf="@+id/missionname"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/missionname"
            android:layout_width="233dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginTop="16dp"
            android:gravity="end"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/flightnumber" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Launch Date"
            android:textColor="@android:color/black"
            app:layout_constraintBaseline_toBaselineOf="@+id/launchdate"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/launchdate"
            android:layout_width="234dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginTop="16dp"
            android:gravity="end"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/missionname" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Wikipedia"
            android:textColor="@android:color/black"
            app:layout_constraintBaseline_toBaselineOf="@+id/wikipedia"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
